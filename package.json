{
  "name": "res-order-api",
  "version": "1.0.0",
  "description": "Restaurant Order API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prettier-format": "prettier --config .prettierrc 'src/**/**/*.ts' --write",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "postinstall": "prisma generate",
    "migrate": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "docker": "sh ./scripts/build.sh",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "node dist/src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrunoUemura/res-order-api.git"
  },
  "keywords": [],
  "author": "OtavioHBL",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrunoUemura/res-order-api/issues"
  },
  "homepage": "https://github.com/BrunoUemura/res-order-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0"
  }
}
